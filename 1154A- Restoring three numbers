#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main(){
    long long x1,x2,x3,x4,a,b,c,large=0;
    cin>>x1>>x2>>x3>>x4;
    vector <long long> arr;
    arr.push_back(x1);arr.push_back(x2);arr.push_back(x3);arr.push_back(x4);
    sort(arr.begin(),arr.end());
    large=arr.at(3);
    
    a=large-arr.at(0);
    b=large-arr.at(1);
    c=large-arr.at(2);
    
    cout<<a<<" "<<b<<" "<<c<<" "<<endl;
    return 0;
}
