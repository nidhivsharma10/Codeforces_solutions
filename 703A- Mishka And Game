#include<iostream>
using namespace std;

int main(){
    int n,m=0,c=0,Msum=0,Csum=0;
    cin>>n;
    for(int i=0;i<n;i++){
       cin>>m>>c;
       if(m>c)
            Msum+=1;
        else if(m<c)
            Csum+=1;
    }
    if(Msum>Csum)
        cout<<"Mishka"<<endl;
    else if(Csum>Msum)
            cout<<"Chris"<<endl;
    else
        cout<<"Friendship is magic!^^"<<endl;
    return 0;
}
