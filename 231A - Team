#include<iostream>
using namespace std;
int main(){
    int n,count=0;
    cin>>n;
    int arr[3];
    while(n--){
        int yes=0,t=0;
        while(t<3){
            cin>>arr[t];
            t++;
        }
        for(int i=0;i<3;i++){
            if(arr[i]==1){
                yes++; 
            }
        }
        if(yes>1){
          count++;  
        }
    }
    cout<<count<<endl;
    return 0;
}
