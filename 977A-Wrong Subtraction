By nidhisharma10, contest: Codeforces Round #479 (Div. 3), problem: (A) Wrong Subtraction, Accepted, #, Copy
#include<iostream>
using namespace std;
int main(){
    int num,k, last_digit;
    cin>>num>>k;
    while(k--){
        last_digit=num%10;
        if(last_digit==0)
            num=num/10;
        else
            num=num-1;
    }
    cout<<num;
    return 0;
}
