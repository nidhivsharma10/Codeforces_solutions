

#include <bits/stdc++.h>

using namespace std;

int main()
{
   string str[3];
   for(int i=0;i<3;i++)
    cin>>str[i];
    int k,l=3;
    for(int i=0;i<3;i++){
        l--;k=3;
        for(int j=0;j<3;j++){
            k--;
           if(str[j][i]!=str[k][l]){
               cout<<"NO";
               return 0;
           }
        }
    }
    cout<<"YES";
   
    return 0;
}
